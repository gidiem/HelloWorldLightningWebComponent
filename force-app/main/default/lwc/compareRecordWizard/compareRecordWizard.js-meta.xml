<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata" fqn="compareRecordWizard">
    <apiVersion>45.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
      <target>lightning__HomePage</target>
  </targets>
  <targetConfigs>   
        <targetConfig targets="lightning__HomePage">
            <property name="titleObject" type="String" default="Accounts" label="Plural of object name to compare for title label"/>
            <property name="nameField" type="String" default="Name" label="Name field on the Object"/>
            <property name="iconName" type="String" default="Name" label="SLDS icon name (type:name). i.e. standard:account"/>
            <property name="numOfRecords" type="Integer" default="2" label="Number of records to compare"/>
            <property name="objType" type="String" default="Account" label="Object type to compare"/>
            <property name="fieldSet" type="String" label="Field Set API Name to display"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>